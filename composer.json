{
    "name": "crockerio/elap",
    "description": "Extendable Laravel Admin Panel",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joshua Crocker",
            "email": "joshua+code@crocker.io"
        }
    ],
    "autoload": {
        "classmap": [
            "src/Factories"
        ],
        "psr-4": {
            "Crockerio\\ELAP\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Crockerio\\ELAP\\Tests\\": "tests/"
        }
    },
    "require": {},
    "require-dev": {
        "orchestra/testbench": "^5.3"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Crockerio\\ELAP\\ELAPServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    }
}
